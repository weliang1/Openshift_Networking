apiVersion: v1
kind: Service
metadata:
  name: egress-router-dns-svc
spec:
  ports:
  - name: con1
    protocol: TCP
    port: 80
    targetPort: 80
  - name: con2
    protocol: TCP
    port: 5000
    targetPort: 8000
  - name: con3
    protocol: TCP
    port: 6000
    targetPort: 8001
  - name: con4
    protocol: TCP
    port: 8080
    targetPort: 8080
  - name: con5
    protocol: TCP
    port: 8443
    targetPort: 8443
  type: ClusterIP
  selector:
    name: egress-router-dns-pod
